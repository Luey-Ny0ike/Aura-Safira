<div class="container-fluid testimonials">
  <h3 id="say">What our clients say</h3>
  <br><br>
    <!-- testimonials will go here -->
    <div class="container actual">
      <% if @testimonials.any? %>
      <div class="slide row">
        <% @testimonials.each do|testimonial|%>
          <div class="col-md-12">
          <h3 id="testi">"<%= testimonial.testimony%>"</h3>
          <h5><%= testimonial.name%> - <%= testimonial.position%></h5>
          </div>
        <% end %>
      </div>
      <% else %>
      <h4>No testimonials, Be the first to add</h4>
      <% end %><br>
  <br>
  <div class="md">
    <!-- Trigger the modal with a button -->
    <button type="button" class="btn btn-info" data-toggle="modal" data-target="#myModal">New testimonial</button>

    <!-- Modal -->
    <div class="modal fade" id="myModal" role="dialog">
      <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            <h4 class="modal-title">Add a new testimonial</h4>
          </div>
          <div class="modal-body">
            <%= render 'testimonials/form'%>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          </div>
        </div>

      </div>
    </div>
  </div>
  </div>
</div>

