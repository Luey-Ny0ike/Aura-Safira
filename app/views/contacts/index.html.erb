<div class="container-fluid nyuma">
  <div class="container row">
    <p id="notice"><%= notice %></p>

  <div class="col-md-1"></div>
  <div class="fom col-md-5">
    <h1 id="kati">Contact Us</h1>
    <p>Reach out to us today and get to learn more about the services we offer</p>
  <br><br>
    <%= render 'contacts/form'%>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Subject</th>
      <th>Message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.email %></td>
        <td><%= contact.subject %></td>
        <td><%= contact.message %></td>
        <td><%= link_to 'Show', contact %></td>
        <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
        <td><%= link_to 'Destroy', contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

